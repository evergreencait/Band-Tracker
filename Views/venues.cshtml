
@if (@Model.Count == 0)
{
  <p>No venues have been added yet!</p>
}
else
{
  <h1>Here is your list of venues:</h1>
  <ul>
    @foreach (var venue in Model)
    {
       <a href="/venue/@venue.GetId()"><li>@venue.GetName()</li></a>
    }
  </ul>
  <form action="/venues/delete" method="post">
    <button type="submit" class="btn">Clear all venues</button>
  </form>
}
<p><a href="/clients/new">Add a new venue</a></p>
<p><a href="/">Return home</a></p>
